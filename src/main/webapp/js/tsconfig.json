{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./app/app.ts",
      "classes/User.ts",
      "./controllers/UserController.ts"

    ],
    "files": [
        "lib/*.d.ts",
        "lib/angular-resource-d.ts"
    ]
}
